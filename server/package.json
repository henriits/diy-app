{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "tsx src",
        "dev": "tsx watch src",
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "migrate:latest": "tsx src/database/migrate/bin",
        "gen:types": "kysely-codegen --camel-case --dialect postgres --out-file src/database/types.ts && prettier --write src/database/types.ts",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "create-migration": "tsx scripts/createMigration.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@tsconfig/node18": "^18.2.4",
        "@types/bcrypt": "^5.0.2",
        "@types/better-sqlite3": "^7.6.10",
        "@types/express": "^4.17.21",
        "@types/lodash": "^4.17.0",
        "@types/node": "^20.12.7",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "@vitest/coverage-v8": "^1.5.2",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "kysely": "^0.27.3",
        "kysely-codegen": "^0.15.0",
        "prettier": "^3.2.5",
        "supertest": "^7.0.0",
        "tslint": "^6.1.3",
        "tsx": "^4.7.3",
        "typescript": "^5.5.4",
        "vitest": "^1.5.2"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "better-sqlite3": "^9.5.0",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "http-status-codes": "^2.3.0",
        "lodash": "^4.17.21",
        "postgres": "^3.4.4",
        "zod": "^3.23.4"
    }
}
